		"sass": "^1.56.2",
		"svelte": "^3.55.0",
		"svelte-check": "^2.10.2",
		"svelte-preprocess": "^4.9.5",
		"svelte2tsx": "^0.5.16",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.1"
