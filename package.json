		"change-case": "^4.1.2",
		"litepicker": "^2.0.12",
		"moment": "^2.29.4",
		"nanoid": "^3.3.4",
		"prism-svelte": "^0.5.0",
		"prismjs": "^1.29.0",
		"swiper": "^8.4.2",
		"tom-select": "^2.1.0"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.0.1",
		"@sveltejs/adapter-static": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
