{
	"name": "@ubeac/svelte",
	"version": "0.0.2",
	"type": "module",
	"svelte": "index.js",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"package": "npm run style && svelte-kit package",
		"publish": "npm run package && npm publish ./package",
		"start": "npm run style && svelte-kit dev",
		"sb": "npm run storybook:start",
		"sb:build": "storybook:build",
		"storybook:start": "npm run style && start-storybook -p 6006",
		"storybook:build": "npm run style && build-storybook -o ./.storybook/build",
		"style": "node scripts/styles.generator.js"
	},
	"dependencies": {
		"@googlemaps/js-api-loader": "^1.14.3",
		"@iconify/svelte": "^2.2.1",
		"@popperjs/core": "^2.11.6",
		"@storybook/addon-storysource": "^6.5.10",
		"@tabler/core": "^1.0.0-beta11",
		"bootstrap": "^5.1.3",
		"change-case": "^4.1.2",
		"litepicker": "^2.0.12",
		"moment": "^2.29.4",
		"nanoid": "^3.3.4",
		"tom-select": "^2.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@storybook/addon-actions": "^6.5.1",
		"@storybook/addon-essentials": "^6.4.22",
		"@storybook/addon-links": "^6.5.10",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/addon-svelte-csf": "^1.0.0",
		"@storybook/builder-vite": "^0.1.37",
		"@storybook/svelte": "^6.1.21",
		"@sveltejs/adapter-auto": "^1.0.0-next.64",
		"@sveltejs/kit": "1.0.0-next.405",
		"@trivago/prettier-plugin-sort-imports": "^3.2.0",
		"@tsconfig/svelte": "^3.0.0",
		"@types/bootstrap": "^5.1.10",
		"@types/cookie": "^0.5.1",
		"@types/google.maps": "^3.49.2",
		"@types/luxon": "^2.4.0",
		"autoprefixer": "^10.4.8",
		"babel-loader": "^8.2.5",
		"postcss-cli": "^10.0.0",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.54.5",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.1",
		"svelte-loader": "^3.1.3",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.14",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.9"
	}
}
